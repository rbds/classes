%% MAE 6254 HW1
% Randy Schur
% 2/8/16
close all

d1 = -2:1:2;
d2 = -2:1:2;

[X,Y] = meshgrid(d1, d2);
for i= d1
    for j= d2
        x0 = [i; j];

        [t, x] = ode45(@HW2_1, [0, 10], x0);

        x = x(:,1);
        xdot = x(:,2);

        hold on
%         quiver(X,Y,theta, theta_dot)
        plot(x, xdot, 'b')
    end
end


